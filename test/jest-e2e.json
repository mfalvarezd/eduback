{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "testEnvironment": "node",
  "testRegex": ".spectest.ts$",
  "testTimeout": 30000,
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1",
    "^prisma/(.*)$": "<rootDir>/prisma/$1"
  },
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
